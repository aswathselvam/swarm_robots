<?xml version="1.0"?>
<launch>
	<arg name="x" default="0" />
	<arg name="y" default="0" />
	<arg name="z" default="1" />
	<arg name="yaw" default="0" />
	<arg name="joystick" default="false" />
	<arg name="model_name" default="jackal0" />
	<!-- <arg name="config" default="base" /> -->
	<arg name="namespace" default="jackal0"/>
	<arg name="env_runner" value="$(eval 'env_run' if not optenv('OS', 'unknown').lower().startswith('windows') else 'env_run.bat')" />
	
  <!-- 08th December load robot description from jackal xaco file  \xacro\swarm_jackal.xacro -->
	<!-- <param name="robot_description"
         command="$(find jackal_description)/scripts/$(arg env_runner)
                    $(find jackal_description)/urdf/configs/$(arg config)
                    $(find xacro)/xacro $(find jackal_description)/urdf/jackal.urdf.xacro
                    hyphen hyphen inorder" /><node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="screen"/> -->
	<!-- Configuration of Jackal which you would like to simulate.
       See jackal_description for details. -->
	<!-- Load Jackal's description, controllers, and teleop nodes. -->
	<!-- <include file="$(find jackal_description)/launch/description.launch"><arg name="config" value="$(arg config)" /></include> -->
	
  <include file="$(find jackal_control)/launch/control.launch" />
	<!-- Spawn Jackal in Gazebo package -->
	<node name="jackal_spawner" pkg="gazebo_ros" type="spawn_model"
	args="-urdf -param /$(arg ns)/robot_description -model $(arg robot_name) -x $(arg x) -y $(arg y) -z $(arg z) -R 0 -P 0 -Y $(arg yaw)"  
  respawn="false" output="screen"/>
</launch>
